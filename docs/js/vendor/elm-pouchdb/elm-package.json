{
    "version": "1.0.5",
    "summary": "Elm bindings to the great Pouchdb library.",
    "repository": "https://github.com/powet/elm-pouchdb.git",
    "license": "BSD3",
    "source-directories": [
        "./src/",
        "./src/Native/"
    ],
    "exposed-modules": [
